        </div>
    </div>

    <script>
        function initMap() {
            var uluru = { lat: 10.80585, lng: 106.646031 };
            var map = new google.maps.Map(document.getElementById('map'), {
                zoom: 16,
                center: uluru
            });
            var marker = new google.maps.Marker({
                position: uluru,
                map: map
            });
        }
    </script>
    <script async defer src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCfKiIahLtG7xVbhDmQt3-00hyXHyyQyYs&callback=initMap"></script>
